{
  "name": "nodesio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/verify-imports.js && next build",
    "start": "next start",
    "server": "node server.js",
    "production": "npm run build && npm run server",
    "deploy": "node deploy-server.js",
    "deploy:safe": "npm install --production && npm run build && node server.js",
    "deploy:azure": "node azure-startup.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "postbuild": "next-sitemap",
    "analyze": "ANALYZE=true next build",
    "verify-imports": "node scripts/verify-imports.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "next": "^16.0.10",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@sentry/nextjs": "^10.19.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "24.0.7",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.8",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.8",
    "isomorphic-dompurify": "^2.29.0",
    "jose": "^6.1.0",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "zod": "^4.1.12"
  }
}
